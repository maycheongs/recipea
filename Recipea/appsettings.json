{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/app/data/recipea.db"
  },
  "Spoonacular": {
    "ApiKey": "YOUR_SPOONACULAR_API_KEY"
  },
  "AWS": {
    "Enabled": false,
    "AccessKey": "YOUR_AWS_ACCESS_KEY",
    "SecretKey": "YOUR_AWS_SECRET_KEY",
    "Region": "us-east-1",
    "BucketName": "recipea-images"
  }
}
